    <style>
        .flex-container {
              display: flex;
        flex-wrap: wrap;
        justify-content: space-between;
          margin: 0 10px 10px 0;
        }

    .element {
    box-sizing: border-box;
    width: calc(50% - 20px); /* Adjust the width based on the number of elements per row */
          margin: 0 10px 10px 0;
}

.element:hover .fill-red {
    fill: red; 
    transition: fill 0.3s ease; 
    cursor: pointer;
    position: relative; 
    z-index: 1; 
}

.heart-icon:hover {
    fill: red; 
    z-index: 1; 
}


    </style>
</head>
